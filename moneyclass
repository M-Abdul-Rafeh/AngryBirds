package AngryBirds;

import java.awt.Rectangle;

public class Money {

	public int recW;
	   public int recH;
	   public double xpos;				
	   public double ypos;
	   public boolean isAlive;
	   
	   public Rectangle rec;

	   
	public Money(int pXpos, int pYpos) {
		xpos = pXpos;
		ypos = pYpos;
		recW=50;
		recH=50;
		rec=new Rectangle((int)xpos, (int)ypos, recW, recH);
	}

}
